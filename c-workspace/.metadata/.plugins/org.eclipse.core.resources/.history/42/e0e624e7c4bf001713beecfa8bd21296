#include <iostream.h>
#include <iostream>

void func()
{
	using namespace std;
	cout<<123123<<endl;
	char name[50];
	cin>>name;
	cout<<999999<<endl;

	char addr[50];
	cin>>addr;
	cout<<name:<name<<endl;
	cout<<addr:<<addr<<endl

}
int main()
{
	int n,s;
	n=101;
	s=-1;
	func(n,&s);
	printf("%d",s);
	return 0;
}

