package day2;

import java.util.Random;

public class Double_chromosphere {
	public static void main(String[] args) {
		
		
		for(int i=0;i<6;i++){
			System.out.println(getNum(1,33));
		}
		System.out.println(getNum(1,16));
		
	}
	static int getNum(int low,int high){
		return (int) (Math.random()*high+1);
	}
}
