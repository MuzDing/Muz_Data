package day1;

public class question_one {
	public static void main(String[] args) {
		boolean flag=true;
		for (int i = 2; i <= 100; i++) {
			for (int j = 2; j <= Math.sqrt(i) ; j++) {
				if (i % j == 0) {
					flag=flase;
					break;
				}
			}
			if (flag) {
				System.out.println(i);
				flag=false;
			}
		}
	}
}
