	}
	public static Double add(Double num1,Double num2){
		return num1+num2;
	}
	public static Double jian(Double num1,Double num2){
		return num1-num2;
	}
	public static Double cheng(Double num1,Double num2){
		return num1*num2;
	}
	public static Double chu(Double num1,Double num2){
		return num1/num2;
	}

}
